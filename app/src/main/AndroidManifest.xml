<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.csexamreviewer">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CSExamReviewer"
        tools:targetApi="31">

        <activity android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".NotesActivity" />
        <activity android:name=".ExamActivity" />
        <activity android:name=".NotesDetailActivity" />
        <activity android:name=".QuestionActivity" />

        <activity android:name=".ProfessionalCategoryActivity" />
        <activity android:name=".SubProfessionalCategoryActivity" />

        <activity android:name=".VerbalDiscussionActivity" />
        <activity android:name=".NumericalDiscussionActivity" />
        <activity android:name=".AnalyticalDiscussionActivity" />
        <activity android:name=".ClericalDiscussionActivity" />

        <activity android:name=".VerbalQuestionnaireActivity" />
        <activity android:name=".NumericalQuestionnaireActivity" />
        <activity android:name=".AnalyticalQuestionnaireActivity" />
        <activity android:name=".ClericalQuestionnaireActivity" />
  
        <activity android:name=".SubVerbalDiscussionActivity" />
        <activity android:name=".SubNumericalDiscussionActivity" />
        <activity android:name=".SubGenInfoDiscussionActivity" />
        <activity android:name=".SubClericalDiscussionActivity" />

        <activity android:name=".SubVerbalQuestionnaireActivity" />
        <activity android:name=".SubNumericalQuestionnaireActivity" />
        <activity android:name=".SubGenInfoQuestionnaireActivity" />
        <activity android:name=".SubClericalQuestionnaireActivity" />

    </application>

</manifest>
